<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.*?>
<?import java.lang.Double?>
<?import javafx.collections.FXCollections?>
<StackPane fx:id="stack" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane prefHeight="400.0" prefWidth="600.0">
        <fx:define>
            <Double fx:id="DEFALUT_WIDTH" fx:value="300.0"/>
            <Double fx:id="DEFALUT_HEIGHT" fx:value="50.0"/>
        </fx:define>
        <rowConstraints>
            <RowConstraints minHeight="10.0" percentHeight="20.0" prefHeight="30.0" />
            <RowConstraints minHeight="10.0" percentHeight="60.0" prefHeight="30.0" />
            <RowConstraints minHeight="10.0" percentHeight="20.0" prefHeight="30.0" />
        </rowConstraints>
        <columnConstraints>
            <ColumnConstraints minWidth="10.0" percentWidth="100.0" prefWidth="100.0" />
        </columnConstraints>
        <children>
            <Button fx:id="faq" text="FAQ" GridPane.rowIndex="0" GridPane.valignment="CENTER" GridPane.halignment="LEFT" maxHeight="30" maxWidth="100"></Button>
            <ListView fx:id="addr" maxHeight="$DEFALUT_HEIGHT" maxWidth="$DEFALUT_WIDTH" minHeight="$DEFALUT_HEIGHT" minWidth="$DEFALUT_WIDTH" prefHeight="$DEFALUT_HEIGHT" prefWidth="$DEFALUT_WIDTH" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
                <GridPane.margin>
                    <Insets bottom="$DEFALUT_HEIGHT" />
                </GridPane.margin>
            </ListView>
            <Button fx:id="connect" maxHeight="$DEFALUT_HEIGHT" maxWidth="$DEFALUT_WIDTH" minHeight="$DEFALUT_HEIGHT" minWidth="$DEFALUT_WIDTH" mnemonicParsing="false" prefHeight="$DEFALUT_HEIGHT" prefWidth="$DEFALUT_WIDTH" text="Подключиться" textAlignment="CENTER" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
                <GridPane.margin>
                    <Insets top="70.0" />
                </GridPane.margin>
            </Button>
            <ProgressBar fx:id="loading" visible="false" GridPane.valignment="CENTER" GridPane.rowIndex="1" maxWidth="$DEFALUT_WIDTH" minWidth="$DEFALUT_WIDTH" prefWidth="$DEFALUT_WIDTH" maxHeight="$DEFALUT_HEIGHT" minHeight="$DEFALUT_HEIGHT" prefHeight="$DEFALUT_HEIGHT" GridPane.halignment="CENTER">
                <GridPane.margin>
                    <Insets top="200.0" />
                </GridPane.margin>
            </ProgressBar>
        </children>
    </GridPane>

</StackPane>